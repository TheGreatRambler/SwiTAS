{
	"version": "2.0.0",
	"tasks": [{
			"label": "Build Sysmodule Debug",
			"type": "shell",
			"promptOnClose": true,
			"command": "make DINFO=1",
			"group": "build",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			},
			"problemMatcher": [{
				"base": "$gcc",
				"fileLocation": [
					"autoDetect",
					"${workspaceFolder}"
				]
			}]
		},
		{
			"label": "Build Sysmodule Release",
			"type": "shell",
			"promptOnClose": true,
			"command": "make",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			},
			"problemMatcher": [{
				"base": "$gcc",
				"fileLocation": [
					"autoDetect",
					"${workspaceFolder}"
				]
			}],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Clean",
			"type": "shell",
			"promptOnClose": true,
			"command": "${workspaceRoot}/.vscode/scripts/clean.sh",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			},
			"problemMatcher": [{
				"base": "$gcc",
				"fileLocation": [
					"autoDetect",
					"${workspaceFolder}"
				]
			}]
		}
	]
}