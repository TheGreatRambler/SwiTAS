{
	"version": "2.0.0",
	"tasks": [{
			"label": "Build PC Application",
			"type": "shell",
			"command": "make",
			"presentation": {
				"reveal": "always",
				"panel": "dedicated"
			},
			"problemMatcher": [{
				"base": "$gcc",
				"fileLocation": ["autoDetect", "${workspaceFolder}"]
			}],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Force Build PC Application",
